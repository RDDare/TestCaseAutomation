{
  "manifestVersion": 1,
  "id": "Test-Case-Automation-Task",
  "name": "Test Case Automation",
  "version": "0.0.1",
  "publisher": "true-vaytsekhovskiy",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
