    {
      "path": "icon.png",
      "packagePath": "icon.png"
    },
    {
      "path": "task.json",
      "packagePath": "task.json"
    }
  ],
  "contributions": [
    {
      "id": "test-case-automation-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "TestCaseAutomation"
      }
    }
